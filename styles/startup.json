{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"title": "Startup",
	"settings": {
		"color": {
			"palette": [
				{
					"name": "Primary",
					"slug": "primary",
					"color": "#454DFF"
				},
				{
					"name": "Main",
					"slug": "main",
					"color": "#171A1F"
				},
				{
					"name": "Secondary",
					"slug": "secondary",
					"color": "#405C7A"
				},
				{
					"name": "Main Accent",
					"slug": "main-accent",
					"color": "#B6C7D9"
				},
				{
					"name": "Primary Accent",
					"slug": "primary-accent",
					"color": "#DBDDFF"
				},
				{
					"name": "Tertiary",
					"slug": "tertiary",
					"color": "#F8F7F9"
				},
				{
					"name": "Border Light",
					"slug": "border-light",
					"color": "#DADEE3"
				},
				{
					"name": "Border Dark",
					"slug": "border-dark",
					"color": "#444B57"
				},
				{
					"name": "Base",
					"slug": "base",
					"color": "#fff"
				}
			]
		},
		"spacing": {
			"spacingSizes": [
				{
					"name": "Small",
					"size": "clamp(.5rem, 2.5vw, 1rem)",
					"slug": "small"
				},
				{
					"name": "Medium",
					"size": "clamp(1.5rem, 4vw, 2rem)",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "clamp(2rem, 5vw, 3rem)",
					"slug": "large"
				},
				{
					"name": "Extra Large",
					"size": "clamp(3rem, 7vw, 5rem)",
					"slug": "x-large"
				},
				{
					"name": "2xl",
					"size": "clamp(4rem, 9vw, 7rem)",
					"slug": "xx-large"
				},
				{
					"name": "3xl",
					"size": "clamp(5rem, 12vw, 9rem)",
					"slug": "xxx-large"
				},
				{
					"name": "4xl",
					"size": "clamp(6rem, 14vw, 13rem)",
					"slug": "xxxx-large"
				}
			],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		}
	},
	"styles": {
		"blocks": {
			"core/details": {
				"css": ".wp-block-details summary { font-weight: var(--wp--custom--font-weight--semi-bold) }"
			}
		},
		"elements": {
			"button": {
				"spacing": {
					"padding": {
						"top": ".6em",
						"right": "1.4em",
						"bottom": ".6em",
						"left": "1.4em"
					}
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--expanded)",
					"fontWeight": "var(--wp--custom--font-weight--medium)",
					"lineHeight": "var(--wp--custom--line-height--snug)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--medium)",
			"padding": {
				"top": "0",
				"right": "var(--wp--preset--spacing--medium)",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--medium)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--primary)",
			"fontSize": "var(--wp--preset--font-size--base)",
			"fontWeight": "var(--wp--custom--font-weight--regular)",
			"lineHeight": "var(--wp--custom--line-height--body)"
		}
	}
}
